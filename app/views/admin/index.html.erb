<div class='container-fluid bg-3'>
  <% if @members.any? %>
  <ul>
    <% @members.each do |member| %>
      <li> <%= member.name %> </li>
    <% end %>
  </ul>
  <% else %>
    No members have been added yet.
  <% end %>
  <span class="glyphicon glyphicon-plus"></span><a href='/members/new'>Add a new member</a>
</div>
