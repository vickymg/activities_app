<%= form_for [@member, @activity] do |f| %>
  <%= f.label :name %>
  <%= f.select :name, options_for_select([['Running'], ['Cycling'], ['Walking'],['Kayaking'], ['Swimming']], :selected => :running) %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.submit 'Add activity' %>
<% end %>
